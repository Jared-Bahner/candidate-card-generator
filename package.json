{
  "name": "candidate-card-generator",
  "private": false,
  "version": "1.0.0",
  "description": "AI-powered candidate card generator with resume parsing and PDF export capabilities",
  "type": "module",
  "keywords": [
    "react",
    "ai",
    "resume",
    "pdf",
    "candidate",
    "recruitment",
    "openai"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/candidate-card-generator.git"
  },
  "homepage": "https://your-app-url.vercel.app",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "clean": "rimraf dist node_modules/.vite",
    "prebuild": "npm run clean",
    "postbuild": "echo 'Build completed successfully!'"
  },
  "dependencies": {
    "@react-pdf/renderer": "^4.3.0",
    "lucide-react": "^0.511.0",
    "openai": "^5.0.1",
    "pdfjs-dist": "^4.0.379",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "buffer": "^6.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^13.24.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.32",
    "prettier": "^3.2.5",
    "process": "^0.11.10",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.0",
    "terser": "^5.28.1",
    "util": "^0.12.5",
    "vite": "^5.0.8",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
